<ul class="ulposts">
  {{#each data}}
    <li class="liposts">
      <div class="post">
        <div class="titlepost">
          <h1 class="namepost">{{title}}</h1>
          <h3 class="datepost"> {{dateCreate}}</h3>
        </div>
        <div class="authorpost">
          <h3>{{email}}</h3>
        </div>
        <div class="content">
          {{{body}}}
        </div>
        
        <div class="action" style="display: flex;">
          <form action="/manager/readPost/{{_id}}">
            <button type="submit" class="btn btn-primary">Read</button>
          </form>
        </div>
      </div>
    </li>
  {{/each}}
</ul>